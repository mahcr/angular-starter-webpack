{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "Angular 2 stater kit using webpack",
  "scripts": {
    "prestart": "node config/scripts/create-mockdata.js",
    "start": "webpack-dev-server --inline --progress --port 8080  --host 0.0.0.0",
    "build": "rimraf dist && webpack --config config/webpack.config.prod.js --progress --profile --bail",
    "tslint": "tslint",
    "sass": "node-sass src/app/theme -o src/app/theme"
  },
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
